@model DeportesWeb.Models.RegistryFrequency

@{
    ViewBag.Title = "Eliminar";
}

<h2>Eliminar</h2>

<h3>¿Seguro que quieres eliminar esta Frecuencia?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <!--<dt>
            @Html.DisplayFor(model => model.id)
        </dt>
            -->
        <dd>
            @Html.DisplayFor(model => model.frecuencia)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Regresar", "IndexRegistryFrequecy", new { id = Model.idSportUser})
        </div>
    }
</div>
